<?xml version="1.0" encoding="UTF-8"?>
<addons>
    <addon id="plugin.video.kralex" name="Alem-i İslam" version="1.2.3" provider-name="kralex">
        <requires>
            <import addon="xbmc.python" version="2.1.0" />
            <import addon="repository.mutebessim" version="1.0.1" />
            <import addon="script.module.urlresolver" version="3.0.27" />
            <import addon="plugin.video.youtube" version="5.3.0" />
            <import addon="plugin.program.autocompletion" version="1.0.1" />
        </requires>
        <extension point="xbmc.python.pluginsource" provides="video" library="kralex.py">
            <provides>video</provides>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Alem-i İslam - Kodi için online dini içerik eklentisi</summary>
            <description>Alem-i İslam eklentisi ile internetteki muhtelif dini içerikli sitelerin sohbet, vaaz ve konferans videoları cihazına geliyor.</description>
            <platform>all</platform>
            <assets>
                <icon>icon.png</icon>
                <fanart>fanart.jpg</fanart>
                <screenshot>resources/screenshots/screenshot-01.jpg</screenshot>
                <screenshot>resources/screenshots/screenshot-02.jpg</screenshot>
                <screenshot>resources/screenshots/screenshot-03.jpg</screenshot>
                <screenshot>resources/screenshots/screenshot-04.jpg</screenshot>
            </assets>
        </extension>
    </addon>
    <addon id="repository.mutebessim" name="Mutebessim - Depo" version="1.0.1" provider-name="kralex">
        <extension point="xbmc.addon.repository" name="mutebessim eklentiler">
            <info compressed="false">https://github.com/mutebessim/kodi/raw/master/addons/addons.xml</info>
            <checksum>https://github.com/mutebessim/kodi/raw/master/addons/addon.xml.md5</checksum>
            <datadir zip="true">https://github.com/mutebessim/kodi/raw/master/addons/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Kodi Depom</summary>
            <description>Kodi için hazırladığımız eklentilerin dosyalarına erişebileceğiniz depomuz.</description>
            <platform>all</platform>
        </extension>
    </addon>
    <addon id="repository.mutebessim-aarch64" name="Mutebessim - aarch64 Services - Depo" version="1.0.0" provider-name="kralex">
        <extension point="xbmc.addon.repository" name="mutebessim eklentiler">
            <info compressed="false">https://raw.githubusercontent.com/mutebessim/service-aarch64/master/addons/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/mutebessim/service-aarch64/master/addons/addon.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/mutebessim/service-aarch64/master/addons/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Kodi Depom</summary>
            <description>Kodi için hazırladığımız aarch64 hizmet eklentilerin dosyalarına erişebileceğiniz depomuz.</description>
            <platform>all</platform>
        </extension>
    </addon>
    <addon id="repository.mutebessim-arm" name="Mutebessim - arm Services - Depo" version="1.0.0" provider-name="kralex">
        <extension point="xbmc.addon.repository" name="mutebessim eklentiler">
            <info compressed="false">https://raw.githubusercontent.com/mutebessim/service-arm/master/addons/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/mutebessim/service-arm/master/addons/addon.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/mutebessim/service-arm/master/addons/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Kodi Depom</summary>
            <description>Kodi için hazırladığımız arm mimarisi için hizmet eklentilerin dosyalarına erişebileceğiniz depomuz.</description>
            <platform>all</platform>
        </extension>
    </addon>
<addon id="script.module.urlresolver" name="URLResolver" version="5.0.20" provider-name="Eldorado, tknorris, jsergio">
	<requires>
		<import addon="xbmc.python" version="2.1.0" />
		<import addon="plugin.video.youtube" optional="true" />
		<import addon="script.module.python.twitch" optional="true" />
		<import addon="script.module.cryptopy" version="1.2.6" optional="true" />
	</requires>
	<extension point="xbmc.python.module" library="lib" />
	<extension point="xbmc.python.pluginsource" library="lib/default.py" />
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="en">Resolve common video host URL's to be playable in XBMC/Kodi.</summary>
		<summary lang="hr">Prilagodi najčešće URL-ove video izvora kako bi radili u XBMC/Kodiju.</summary>
		<description lang="en">Resolve common video host URL's to be playable in XBMC/Kodi, simplify addon development of video plugins requiring multi video hosts.</description>
		<description lang="hr">Prilagodi najčešće URL-ove video izvora kako bi radili u XBMC/Kodiju, pojednostavnjuje razvoj priključaka za video dodatke koji zahtjevaju višestruke video izvore.</description>
        <source>https://github.com/jsergio123/script.module.urlresolver</source>
	</extension>
</addon>

