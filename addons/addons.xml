        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>trtarsiv.com'dan TRT Arşivi</summary>
            <description></description>
            <platform>all</platform>
            <assets>
                <icon>icon.png</icon>
                <fanart>fanart.jpg</fanart>
            </assets>
        </extension>
    </addon>
    <addon id="plugin.video.trt" name="TRT TV" version="1.1.0" provider-name="kralex">
